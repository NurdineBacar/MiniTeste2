package InterfacesGraficas;

public class ObjectoCalculadora {
	private double valor1;
	private String operador;
	private double valor2, resultado;
	public ObjectoCalculadora(double valor1, String operador, double valor2, double resultado) {
		super();
		this.valor1 = valor1;
		this.operador = operador;
		this.valor2 = valor2;
		this.resultado = resultado;
	}
	public double getValor1() {
		return valor1;
	}
	public void setValor1(double valor1) {
		this.valor1 = valor1;
	}
	public String getOperador() {
		return operador;
	}
	public void setOperador(String operador) {
		this.operador = operador;
	}
	public double getValor2() {
		return valor2;
	}
	public void setValor2(double valor2) {
		this.valor2 = valor2;
	}
	public double getResultado() {
		return resultado;
	}
	public void setResultado(double resultado) {
		this.resultado = resultado;
	}
	
	
	public String toString() {
		return "ObjectoCalculadora [valor1=" + valor1 + ", operador=" + operador + ", valor2=" + valor2 + ", resultado="
				+ resultado + "]";
	}
	
	
}
