import java.util.Scanner;

public class Teste2 {
public static void main(String[] args) {
	
	Scanner x= new Scanner(System.in);
	
	System.out.print("Digite um numero: ");
	int num= x.nextInt();
	 int factorial= num;
	
	 while(num>1) {
		factorial=factorial*(factorial-1); 
		System.out.println(factorial);
		num--;
	 }
	 
	
}
}
